developer="http://indiecomputing.com/"
url="http://ubos.net/"
maintainer=$developer
pkgname=$(basename $(pwd))
pkgver=0.1
pkgrel=1
pkgdesc="Records web app interactions and generates webapptest tests"
arch=('any')
license=('AGPL3')
depends=(
    'ubos-perl-utils'
)
options=('!strip')
_vendor_perl=$(perl -V::vendorarch: | sed -e "s![' ]!!g")

package() {
# Code
    install -D -m755 ${startdir}/bin/* -t ${pkgdir}/usr/bin/
}
